{
    "name": "indian-sign-language-learning-app",
    "version": "0.1.0",
    "description": "Major Project : Sign Language",
    "main": "index.js",
    "authors": "Mayank Maurya, Saubhagya k. Dev, Anshul Bhatt, Naman Asawa",
    "authors-initials": "MM, SkD, AB, NA",
    "license": "MIT",
    "private": true,
    "scripts": {
        "gulp": "gulp",
        "gulp-build": "gulp build",
        "electrorun": "yarn run gulp-build && electron ./Build/",
        "typescript": "gulp bundle-main",
        "live-run": "node live-server.js",
        "live-run-back": "node live-server-back.js",
        "gulp-watch": "gulp watch-run --live",
        "webrun": "yarn run gulp-build && concurrently \"yarn run live-run\" \"yarn run gulp-build\"",
        "live-webrun": "yarn run gulp-build && concurrently \"yarn run live-run\" \"yarn run gulp-watch\"",
        "live-electrorun": "yarn run gulp-build && concurrently \"yarn run electrorun\" \"yarn run gulp-watch\"",
        "Force": "yarn run gulp-build && concurrently \"yarn run live-run-back\" \"yarn run electrorun\" \"yarn run gulp-watch\"",
        "package": "yarn run gulp-build && electron-packager ./Build/ --overwrite --win --platform=win32 --arch=x64 --icon=src\\assets\\icon.ico --prune=true",
        "msi-package": "yarn run gulp-build && electron-packager ./Build/ --overwrite --win --platform=win32 --arch=x64 --icon=src\\assets\\icon.ico --prune=true"
    },
    "dependencies": {
